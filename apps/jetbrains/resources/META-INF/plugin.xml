<idea-plugin>
  <id>atom-one</id>
  <name>Atom One Theme</name>
  <version>0.2</version>
  <vendor url="https://subtheme.pro">Subtheme</vendor>

  <description>
    <![CDATA[
      The Atom One theme, adapted for JetBrains by <a href="https://subtheme.pro">Subtheme</a>.
      <br/><br/>
      Contribute with GitHub: <a href="https://github.com/subtheme-pro/atom-one">https://github.com/subtheme-pro/atom-one</a>
      <br/><br/>
      <img src="https://raw.githubusercontent.com/subtheme-pro/atom-one/master/apps/jetbrains/screenshots/atom-one-dark-small.png" border="0" width="320">
    ]]>
  </description>

  <idea-version since-build="191.0"/>

  <depends>com.intellij.modules.lang</depends>

  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="246a798b-a742-42c0-a587-43f3aceb11bb" path="/atom-one-dark.theme.json"/>
  </extensions>
</idea-plugin>